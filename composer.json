{
    "name": "helmich/ts-parser",
    "description": "Parser for the TYPO3 configuration language TypoScript.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/martin-helmich",
    "repositories": [
        { "type": "vcs", "url": "file:///home/mhelmich/Git/Privat/typo3-typoscript-parser" }
    ],
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "m.helmich@mittwald.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/martin-helmich/ts-parser/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4",
        "symfony/console": "2.4.*",
        "symfony/dependency-injection": "2.4.*",
        "symfony/config": "2.4.*",
        "symfony/yaml": "2.4.*",
        "helmich/typo3-typoscript-parser": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1.0"
    },
    "bin": [
        "ts-parse"
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "Helmich\\TypoScriptLint": "src/"
        }
    }
}
